{
  "name": "cxn",
  "description": "Network connection module with online/offline events",
  "version": "0.5.0",
  "homepage": "https://github.com/ryanve/cxn",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": [
    "connection",
    "detection",
    "network",
    "offline",
    "online",
    "mobile",
    "detect",
    "browser",
    "ender",
    "ux"
  ],
  "main": "./cxn.js",
  "scripts": {
    "lint": "jshint cxn.js && jshint test.js",
    "vendor": "browserify -r aok -o vendor.js",
    "prestart": "npm update",
    "start": "npm run vendor",
    "pretest": "npm run lint",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/cxn.git"
  },
  "bugs": {
    "url": "https://github.com/ryanve/cxn/issues"
  },
  "devDependencies": {
    "aok": "^1.9.0",
    "browserify": "^13.1.1",
    "jshint": "^2.9.3"
  },
  "jshintConfig": {
    "asi": false,
    "boss": true,
    "browser": true,
    "eqnull": true,
    "expr": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxerr": 5,
    "node": true,
    "sub": true,
    "supernew": true,
    "undef": true,
    "unused": true,
    "-W018": false
  }
}
